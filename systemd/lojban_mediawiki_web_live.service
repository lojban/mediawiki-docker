[Unit]
Description=Site/Webserver for mw.lojban.org - Uncached Version

[Service]
Restart=always
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=lojban_mw_web_live
WorkingDirectory=/home/sampre_mw/mediawiki
ExecStart=/home/sampre_mw/mediawiki/run_web.sh -l
ExecStop=/home/sampre_mw/mediawiki/kill_web.sh -l
TimeoutStartSec=10min

[Install]
WantedBy=default.target
